<br>
	<%= link_to "Add new Vehicle", "employee/new", class:'action new', class: "link1"%>
	<br> <br>
	<h1 class="card-title text-center">Parking Management System</h1>
	<table class="listing" summary ="Subject list">
			<tr class="header">
					<th>Cars</th>
					<th>Model Name</th>
					<th>Action</th>
				</tr>
				<% @vec.each do |v|%>
					<tr>
							<td><%= v.number%></td>
							<td><%= v.car_model_name%></td>
							<td class="action">
                  <%= link_to "Show", employee_path(v.id), class:"link" %>
									<%= link_to "Parking Out", edit_employee_path(v.id), class:"link" %>
									<%= link_to "Wrong Entry", employee_path(v.id), class:"link", data: {
                    turbo_method: :delete,
                    turbo_confirm: "Are you sure that the entry is wrong?"
                  } %>
							</td>
					</tr>
			<%end%>
	</table>